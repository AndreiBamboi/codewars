import numpy as np
def comp(array1, array2):
    if array1==None or array2==None:
        return False
    else:
        return False not in [i[0]**2==i[1] for i in zip(np.sort(array1), np.sort(array2))]


a1=[121, 144, 19, 161, 19, 144, 19, 11]
a2=[121, 14641, 20736, 361, 25921, 361, 20736, 361]
comp(a1,a2)

